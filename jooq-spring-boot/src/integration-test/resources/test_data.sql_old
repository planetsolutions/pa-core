INSERT INTO public.system (id, sys_date_cr, sys_desc, sys_date_mod, sys_title, sys_author, sys_modifier, sys_readers, sys_editors, sys_folders, sys_type, sys_version, sys_parent, sys_file_path, sys_file_mime_type, sys_file_length, data, sys_file_name, sys_uuid, sys_symbolic_name, sys_tenant, sys_application, sys_acl, sys_status, sys_parent_uuid) VALUES (25, '2018-05-05 10:30:17.753957', '', '2018-05-11 20:03:11.06', 'Комментарии', NULL, 'boot', '{all}', NULL, NULL, 'tabs_config', '0.1', NULL, NULL, NULL, NULL, '{"query": "/boot/browser/test/root?cmisselector=children&succinct=true&filter=type:eif_comment&objectId=${doc.id}", "en_title": "Comments", "template": " <div class=\"panel panel-default comment-panel\" *ngFor=\"let comm of data\">\n        <div class=\"panel-body\">\n          <div class=\"row\">\n            <div class=\"col-xs-8\">\n              <span class=\"glyphicon glyphicon-comment\"></span>&nbsp;<label>{{comm.data[''subject'']}}</label>\n              <p>{{comm.data[''eif_comment_text'']}}</p>\n              <label class=\"text-muted\">Refers to:</label>&nbsp; {{comm.data[''eif_refers_value'']}}<BR>\n              <label class=\"text-muted\">Context:</label>&nbsp; {{comm.data[''eif_root_ref_context'']}}<BR>\n            </div>\n            <div class=\"col-xs-4\">\n              <span class=\"glyphicon glyphicon-user\"></span>&nbsp;<label>{{comm.data[''r_creator_name'']}}</label><BR>\n              <span class=\"glyphicon glyphicon-time\"></span>&nbsp;<i>{{comm.data[''r_creation_date''] | dateTimeFormat}}</i>\n              <BR><BR>\n              <label class=\"text-muted\">Made on:</label>&nbsp;Rev {{comm.data[''eif_made_on'']}}<BR>\n              <label class=\"text-muted\">Closed on:</label>&nbsp;Rev {{comm.data[''eif_closed_on'']}}<BR>\n            </div>\n          </div>\n          <p *ngIf=\"comm.data[''cmis:contentStreamFileName'']\">\n            <span class=\"glyphicon glyphicon-paperclip\"></span>&nbsp;\n            <a href=\"javascript://\" (click)=\"api.downloadDocument(comm.id)\" [title]=\"''document.attachments.download'' | translate\">{{comm.data[''cmis:contentStreamFileName'']}}</a>\n            / <em>{{comm.data[''cmis:contentStreamLength''] | fileSize}}</em>\n          </p>\n        </div>\n      </div>", "symbolicName": "comments"}', NULL, '824eebd9-b19e-48dd-aa2c-0a53b4937512', 'comments', NULL, NULL, '{"full": ["admins"], "read": ["all"], "delete": [], "edit_prop": [], "view_prop": ["all"], "change_content": [], "modify_security": []}', NULL, NULL);
